{
  "name": "obsidian-linter",
  "version": "1.3.7",
  "description": "Enforces consistent markdown styling for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js --environment BUILD:production && terser main.js --compress toplevel=true,passes=2 --ecma 2016 --comments \"/(GENERATED|Copyright|license)/\" --output main.js",
    "test": "jest",
    "test-suite": "jest -t \"$1\"",
    "docs": "node docs.js",
    "compile": "npm run build && npm run docs && npm run lint && npm run test",
    "lint": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/diff": "^5.0.2",
    "@types/diff-match-patch": "^1.0.32",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^28.1.3",
    "babel-plugin-import-glob": "^2.0.0",
    "eslint": "^8.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "jest": "^28.1.3",
    "moment": "^2.29.4",
    "obsidian": "^0.15.4",
    "rollup": "^2.77.0",
    "rollup-plugin-glob-import": "^0.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.5",
    "diff-match-patch": "^1.0.5",
    "js-yaml": "^4.1.0",
    "loglevel": "^1.8.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "ts-dedent": "^2.2.0",
    "unified": "^10.1.2",
    "unified-lint-rule": "^2.1.0"
  }
}
